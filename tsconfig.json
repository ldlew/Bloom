{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,

    "strict": true,
    "noEmit": true,
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "tests/**/*.ts", 
    "tests/**/*.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
