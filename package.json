{
  "name": "client",
  "version": "1.0.0",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint src __tests__",
    "lint:fix": "eslint src __tests__ --fix",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down",
    "docker:lint": "docker compose run --rm app npm run lint",
    "docker:test": "docker compose run --rm app npm run test"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.1",
    "@react-navigation/elements": "^2.6.5",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "expo": "~54.0.25",
    "expo-font": "^14.0.9",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "^6.0.8",
    "expo-crypto": "~14.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-nitro-modules": "^0.31.7",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-unistyles": "^3.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.37.0",
    "eslint-config-expo": "^10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "react-native-svg-transformer": "^1.5.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}
